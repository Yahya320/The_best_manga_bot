import discord
from discord.ext import commands
import random
import os
import requests

manga = ['Death NoteğŸ“’', 'NarutoğŸŸ ', 'Dungeon MeshiğŸ’', 'Shadows HouseğŸ”´']

intents = discord.Intents.default()
intents.message_content = True

bot = commands.Bot(command_prefix='$', intents=intents)

@bot.event
async def on_ready():
    print(f'We have logged in as {bot.user}')

@bot.command()
async def hello(ctx):
    await ctx.send(f'ĞŸÑ€Ğ¸Ğ²ĞµÑ‚! Ğ¯ Ğ±Ğ¾Ñ‚ {bot.user}!')

@bot.command()
async def mem(ctx):
    img_name = random.choice(os.listdir('images'))
    with open(f'images/{img_name}', 'rb') as f:
        picture = discord.File(f)
   # ĞœĞ¾Ğ¶ĞµĞ¼ Ğ¿ĞµÑ€ĞµĞ´Ğ°Ğ²Ğ°Ñ‚ÑŒ Ñ„Ğ°Ğ¹Ğ» ĞºĞ°Ğº Ğ¿Ğ°Ñ€Ğ°Ğ¼ĞµÑ‚Ñ€!
    await ctx.send(file=picture)

@bot.command()
async def best_manga(ctx):
    await ctx.send(random.choice(manga))

@bot.command()
async def heh(ctx, count_heh = 5):
    await ctx.send("he" * count_heh)

@bot.command()
async def calc(ctx, left: int, right: int):
    """Adds two numbers together."""
    await ctx.send(left + right)

bot.run("")
